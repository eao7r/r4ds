pollutant_dist["CO", "Maritime",city, rr],4),
round((pollutants_monthly_game["SO2",city,end_months[rr]]-
pollutants_monthly_game["SO2",city,end_months[rr-2]]) *
pollutant_dist["SO2", "Maritime",city, rr],4))
